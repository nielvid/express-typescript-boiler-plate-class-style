{
  "name": "express-typescript-boiler-plate",
  "version": "1.0.0",
  "description": "Express App with Typescript boiler-plate",
  "main": "main.js",
  "type": "commonjs",
  "scripts": {
    "build": "rimraf dist && tsc --project src/",
    "start": "concurrently \"tsc --watch\" \"nodemon -q dist/main.js\"",
    "start:dev": "npm start",
    "start:prod": "node dist/main.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test": "jest"
  },
  "keywords": [
    "typescript",
    "express"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:nielvid/express-typescript-boiler-plate.git"
  },
  "author": "Nielvid",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^16.10.1",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "nodemon": "^2.0.12",
    "prettier": "^2.4.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@typegoose/typegoose": "^9.4.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-rate-limit": "^5.4.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.3",
    "mysql2": "^2.3.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.37",
    "winston": "^3.3.3"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
